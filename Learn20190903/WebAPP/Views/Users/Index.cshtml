
<div class="text-center">
    <h1>用户列表</h1>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>用户名</th>
                <th>性别</th>
                <th>出生日期</th>
                <th>创建时间</th>
                <th>状态</th>
            </tr>
        </thead>
        <tbody id="list">
            <tr>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
        </tbody>
    </table>
</div>
<script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
<script type="text/javascript">
    $(function () {
        $.getJSON("GetUsers", function (data) {
            var html = "<tr>"
            $.each(data.Result, function (i, o) {
                html += "<td>" + o.UserName + "</td>";
                html += "<td>" + o.Six + "</td>";
                html += "<td>" + o.Birthday + "</td>";
                html += "<td>" + o.CreateTime + "</td>";
                html += "<td>" + o.Status + "</td>";
            });
            html += "</tr>";

            $("#list").html(html);
        });
    })
</script>